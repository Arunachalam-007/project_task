package com.chainsys.asmt;

public class PolymorphismTest {

	public static void main(String[] args) {
		
		//Overloading
		Polymorphism p=new Polymorphism();
		p.calc(10,20, 30);
		p.calc(5, 10);
		
		//Overriding
		Overriding o=new Overriding();
		o.calc();
		
		OverridingChild oc=new OverridingChild();
		oc.calc();
	}
	
	
	
	

}
