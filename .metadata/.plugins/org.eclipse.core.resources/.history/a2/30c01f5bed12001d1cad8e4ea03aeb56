package file_asmt;

import java.io.File;
import java.io.IOException;

public class FileConcept {

	public static void main(String[] args) throws IOException {
		File f=new File("sample.txt");
		
		if(f.exists()) {
			System.out.println("File exists");
		}
		else {
			f.createNewFile();
		}
		
		
	}
	

}
