package com.chainsys.asmt;

public class InterfaceAsmt implements Interface1,Interface2{

	
	
	//Interface : Interface can have abstract methods and variables.It cannot have a method body.It is 
	//used to achieve abstraction and multiple inheritance.
	
	//A class can implement more than one interface at a time.
	
	

	
	public void subtraction() {
		
		int a=10,b=5;
		int result=a-b;
		System.out.println(result);
	}

	
	public void addition() {
		int d=10,e=20;
		int result1=d+e;
		System.out.println(result1);
		
	}
	
	
	
	
}
