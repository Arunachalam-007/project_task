		Manager m=new Manager("Momo", 20, 327782987l, "Tenkasi", 90000);
		
		System.out.println("name:"+e.name+"age :"+e.age+"phone Number :"+e.phoneNumber+"Address :"+e.address+"Salary :"+e.salary);
		e.printSalary();
