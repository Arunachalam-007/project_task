package com.chainsys.abstraction;

public class MemberTest {

	public static void main(String[] args) {
		Employee e=new Employee("Arun", 21, 72672877898l, "Kovilpatti", 30000);
		Manager m=new Manager("Momo", 20, 327782987l, "Tenkasi", 90000);
		
		System.out.println(e.name+" "+e.age+" "+e.phoneNumber+" "+e.address+" "+e.salary);
		e.printSalary();
		System.out.println(e.specDisplay("Trainee Pgmer"));
		System.out.println();
		System.out.println("**********************************");
		System.out.println(m.name+" "+m.age+" "+m.phoneNumber+" "+m.address+" "+m.salary);
		System.out.println(m.deptDisplay("Production"));

	}

}
